{
  "name": "GeoGuessr timer",
  "version": "0.0.2",
  "description": "This extension adds timers to GeoGuessr score bar",
  "manifest_version": 2,
  "permissions": [],
  "content_scripts": [
    {
      "js": [
        "src/play.js"
      ],
      "matches": [
        "https://geoguessr.com/*/play",
        "https://www.geoguessr.com/*/play"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}